{
  "name": "pixelize-engine",
  "version": "3.0.3",
  "description": "A pixely 2d game engine",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "concurrently \"npm run watch\" \"npm run test-dev\"",
    "watch": "tsc --watch",
    "test-build": "rollup -c rollup.config.test.js",
    "test-dev": "rollup -c rollup.config.test.js -w",
    "site-build": "cd site; npm run build",
    "site-dev": "cd site; npm run dev"
  },
  "keywords": [],
  "author": "NPH",
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "files": [
    "dist/"
  ],
  "types": "dist/types",
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@rollup/plugin-babel": "^5.2.2",
    "@rollup/plugin-commonjs": "^19.0.0",
    "@rollup/plugin-node-resolve": "^11.0.1",
    "concurrently": "^7.0.0",
    "rollup": "^2.35.1",
    "rollup-plugin-terser": "^7.0.2",
    "typescript": "^4.1.5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nph278/pixelize.git"
  },
  "homepage": "https://github.com/nph278/pixelize"
}
